{
  "manifest_version": 3,
  "name": "Tabs Randomizer",
  "version": "1.0.0",
  "description": "Randomizes the order of your browser tabs at regular intervals",
  "permissions": [
    "tabs",
    "storage",
    "scripting"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html"
  }
}

